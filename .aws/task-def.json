{
  "family": "optiquant-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "optiquant-container",
      "image": "123456789012.dkr.ecr.eu-north-1.amazonaws.com/optiquant:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8501,
          "hostPort": 8501
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::123456789012:role/ecsTaskExecutionRole"
}
